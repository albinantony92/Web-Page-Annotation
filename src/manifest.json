{
  "manifest_version": 2,

  "name": "Web Page Annotation",
  "description": "Annotate/Mark on any Webpages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "FullscreenContentSetting" : "allow"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "tabs", "http://www.google.com/*",
    "http://www.google.com/",
    "http://*/"
  ]
}